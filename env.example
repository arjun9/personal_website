# Google Analytics 4 Measurement ID
# Get this from your Google Analytics property settings
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Search Console Verification Code
# Get this from Google Search Console when you add your property
NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=your_verification_code_here

# Your site URL (if not already set)
NEXT_PUBLIC_SITE_URL=https://yourdomain.com

# Twitter API Credentials (OAuth 1.0a - Required for posting tweets)
# Get these from https://developer.twitter.com/en/portal/dashboard
TWITTER_APP_KEY=your_app_key_here
TWITTER_APP_SECRET=your_app_secret_here
TWITTER_ACCESS_TOKEN=your_access_token_here
TWITTER_ACCESS_SECRET=your_access_token_secret_here
